<?xml version="1.0" ?>
<sdf version="1.6">
  <world name="RL_MazeWorld">
    <!-- A global light source -->
    <include>
      <uri>model://sun</uri>
    </include>
    
    <!-- A ground plane -->
    <include>
      <uri>model://ground_plane</uri>
    </include>

    <!-- Ground texture for better visualization -->
    <model name="maze_floor">
      <static>true</static>
      <pose>0 0 0.01 0 0 0</pose>
      <link name="link">
        <visual name="visual">
          <geometry>
            <box>
              <size>8 8 0.02</size>
            </box>
          </geometry>
          <material>
            <ambient>0.8 0.8 0.8 1</ambient>
            <diffuse>0.8 0.8 0.8 1</diffuse>
          </material>
        </visual>
        <collision name="collision">
          <geometry>
            <box>
              <size>8 8 0.02</size>
            </box>
          </geometry>
        </collision>
      </link>
    </model>

    <!-- START MARKER -->
    <model name="start_marker">
      <static>true</static>
      <pose>-2.5 -2.5 0.1 0 0 0</pose>
      <link name="link">
        <collision name="collision">
          <geometry>
            <cylinder>
              <radius>0.4</radius>
              <length>0.2</length>
            </cylinder>
          </geometry>
        </collision>
        <visual name="visual">
          <geometry>
            <cylinder>
              <radius>0.4</radius>
              <length>0.2</length>
            </cylinder>
          </geometry>
          <material>
            <ambient>0 0 0.8 1</ambient>
            <diffuse>0 0 0.8 1</diffuse>
            <emissive>0 0 0.5 1</emissive>
          </material>
        </visual>
      </link>
    </model>

    <!-- GOAL MARKER -->
    <model name="goal_marker">
      <static>true</static>
      <pose>2.5 2.5 0.1 0 0 0</pose>
      <link name="link">
        <collision name="collision">
